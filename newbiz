#l /bin/bash

#create a file with the name domino.yaml
touch domino.yaml

#create a directory with the name odsl-gxp
mkdir odsl-gxp

#print out 3 calender months {previous current and next month}and redirect the output intoi the file calender
cal-3 > calender

#update the package on the machine
sudo apt-get update

#print out job processoing......
"echo" job processing.....

#let the machine sleep for 10 seconds
sleep 10

#Echo the sentence "yaml means yam1 aint markup laanguage" and redirect it into domino yaml file
echo "yaml means yaml aint markup language" > domino.yaml

#echjo "these are the last 5 users created on this machine and append into domino.yaml
echo "these are the last 5 users created on this machine" >> domino.yaml

#to check the users on this machine
cat /etc/passwd  or use tail -5 /etc/passwd to see the last 5 users

#to check the last 5 users create on this machine and append it ino domino.yaml file
tail -5 /etc/passwd >> domino.yaml

#sleep for 5 seconds
sleep 5

